    stages {
        stage("Проверка") {
            steps {
                script {
                    // Преобразование
                    ArrayList<String> allHosts = new ArrayList<>()
                    allHosts.addAll(hosts.split(',\\s*'))

                    // Преобразование
                    ArrayList<String> allPorts = new ArrayList<>()
                    allPorts.addAll(ports.split(',\\s*'))
                    ports = allPorts.join(',')

                    String host
                    int size = allHosts.size()
                    for (int i = 0; i < size; i++) {
                        host = allHosts.get(i)
                        Shell.echo("Проверка ${host}")
                        Shell.echo(Shell.shGetOutput("nmap -Pn -n --reason ${host} -p ${ports} | grep -iE 'port|^[0-9]+/[a-z]+'"))
                    }
                }
            }
        }
    }
